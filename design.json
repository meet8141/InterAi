{
  
  "colorPalette": {
    "primary": {
      "background": "#1a4d4d",
      "surface": "#f5ebe0",
      "surfaceLight": "#faf6f1",
      "accent": "#2d5f5f",
      "accentLight": "#3d7373"
    },
    "secondary": {
      "peach": "#f4cdb8",
      "blush": "#f5ddd1",
      "sage": "#a8b5a8",
      "mint": "#c5d5d0",
      "rose": "#e8b4a8"
    },
    "neutral": {
      "text": "#1f2937",
      "textSecondary": "#4b5563",
      "textMuted": "#6b7280",
      "border": "#e5d5c8",
      "divider": "#f0e6db"
    },
    "botanical": {
      "green": "#4a6b5b",
      "darkGreen": "#2d4a3d",
      "leafGreen": "#5a7d6b",
      "stemGreen": "#3d564a"
    }
  },
  "typography": {
    "fontFamily": {
      "primary": "Playfair Display, Georgia, serif",
      "secondary": "Inter, -apple-system, sans-serif",
      "display": "Playfair Display, serif"
    },
    "scale": {
      "display": {
        "fontSize": "42px",
        "fontWeight": "400",
        "lineHeight": "1.1",
        "letterSpacing": "0.02em",
        "fontFamily": "display"
      },
      "h1": {
        "fontSize": "32px",
        "fontWeight": "400",
        "lineHeight": "1.2",
        "letterSpacing": "0.02em",
        "fontFamily": "display"
      },
      "h2": {
        "fontSize": "24px",
        "fontWeight": "400",
        "lineHeight": "1.3",
        "letterSpacing": "0.01em",
        "fontFamily": "display"
      },
      "h3": {
        "fontSize": "18px",
        "fontWeight": "500",
        "lineHeight": "1.4",
        "fontFamily": "secondary"
      },
      "body": {
        "fontSize": "15px",
        "fontWeight": "400",
        "lineHeight": "1.6",
        "fontFamily": "secondary"
      },
      "small": {
        "fontSize": "13px",
        "fontWeight": "400",
        "lineHeight": "1.5",
        "fontFamily": "secondary"
      },
      "caption": {
        "fontSize": "11px",
        "fontWeight": "500",
        "lineHeight": "1.4",
        "fontFamily": "secondary"
      }
    }
  },
  "spacing": {
    "scale": [0, 4, 8, 12, 16, 20, 24, 32, 40, 48, 64],
    "component": {
      "cardPadding": "20px",
      "sectionPadding": "24px",
      "itemGap": "16px",
      "gridGap": "16px"
    }
  },
  "borderRadius": {
    "sm": "8px",
    "md": "12px",
    "lg": "20px",
    "xl": "28px",
    "2xl": "40px",
    "full": "50%",
    "organic": "40% 60% 50% 50% / 60% 40% 60% 40%"
  },
  "shadows": {
    "sm": "0 2px 8px rgba(0, 0, 0, 0.06)",
    "md": "0 4px 16px rgba(0, 0, 0, 0.08)",
    "lg": "0 8px 24px rgba(0, 0, 0, 0.1)",
    "xl": "0 12px 32px rgba(0, 0, 0, 0.12)",
    "soft": "0 4px 20px rgba(45, 95, 95, 0.08)"
  },
  "layout": {
    "structure": {
      "type": "mobile-first-single-column",
      "maxWidth": "428px",
      "padding": "20px",
      "gap": "24px"
    },
    "screens": {
      "catalog": {
        "sections": ["header", "filterChips", "productGrid", "bottomNav"]
      },
      "basket": {
        "sections": ["header", "cartItems", "orderSummary", "checkoutButton", "bottomNav"]
      },
      "auth": {
        "sections": ["header", "form", "socialLogin"]
      },
      "productDetail": {
        "sections": ["header", "heroImage", "productInfo", "colorSelector", "description", "bottomNav"]
      }
    }
  },
  "components": {
    "header": {
      "background": "primary.accent",
      "height": "80px",
      "padding": "20px",
      "structure": ["title", "actions"],
      "title": {
        "fontSize": "h1",
        "fontFamily": "display",
        "color": "surface",
        "letterSpacing": "0.05em",
        "textTransform": "uppercase"
      },
      "actions": {
        "layout": "horizontal",
        "gap": "16px",
        "iconSize": "24px",
        "iconColor": "surface"
      }
    },
    "productCard": {
      "background": "surface",
      "borderRadius": "xl",
      "overflow": "hidden",
      "structure": ["image", "info"],
      "aspectRatio": "1:1",
      "image": {
        "borderRadius": "organic",
        "overflow": "hidden",
        "aspectRatio": "1:1",
        "objectFit": "cover",
        "position": "relative"
      },
      "cartIcon": {
        "size": "40px",
        "background": "surface",
        "borderRadius": "full",
        "position": "absolute",
        "bottom": "12px",
        "right": "12px",
        "iconSize": "20px",
        "iconColor": "accent",
        "shadow": "md"
      },
      "info": {
        "padding": "16px",
        "textAlign": "center",
        "structure": ["title", "price"]
      },
      "title": {
        "fontSize": "body",
        "fontWeight": "500",
        "color": "text",
        "marginBottom": "4px"
      },
      "price": {
        "fontSize": "small",
        "fontWeight": "400",
        "color": "textSecondary"
      }
    },
    "filterChip": {
      "background": "transparent",
      "border": "1px solid botanical.green",
      "borderRadius": "2xl",
      "padding": "10px 20px",
      "fontSize": "small",
      "fontWeight": "500",
      "color": "botanical.darkGreen",
      "active": {
        "background": "botanical.green",
        "color": "surface"
      }
    },
    "cartItem": {
      "background": "surface",
      "borderRadius": "xl",
      "padding": "16px",
      "structure": ["image", "details", "quantity"],
      "layout": "horizontal",
      "gap": "16px",
      "image": {
        "size": "100px",
        "borderRadius": "organic",
        "objectFit": "cover"
      },
      "details": {
        "flex": "1",
        "structure": ["title", "metadata", "price"]
      },
      "title": {
        "fontSize": "body",
        "fontWeight": "600",
        "color": "text",
        "marginBottom": "4px"
      },
      "metadata": {
        "fontSize": "small",
        "color": "textSecondary",
        "lineHeight": "1.5"
      },
      "price": {
        "fontSize": "h3",
        "fontWeight": "600",
        "color": "accent",
        "marginTop": "8px"
      },
      "quantity": {
        "structure": ["decrementButton", "value", "incrementButton"],
        "layout": "vertical",
        "gap": "8px",
        "alignItems": "center"
      },
      "removeButton": {
        "size": "32px",
        "background": "transparent",
        "color": "textMuted",
        "position": "absolute",
        "top": "8px",
        "right": "8px"
      }
    },
    "quantityControl": {
      "structure": ["decrementButton", "value", "incrementButton"],
      "layout": "horizontal",
      "gap": "12px",
      "alignItems": "center",
      "button": {
        "size": "36px",
        "background": "surface",
        "border": "1px solid neutral.border",
        "borderRadius": "full",
        "iconSize": "16px",
        "color": "text"
      },
      "value": {
        "fontSize": "body",
        "fontWeight": "500",
        "color": "text",
        "minWidth": "32px",
        "textAlign": "center"
      }
    },
    "button": {
      "primary": {
        "background": "accent",
        "color": "surface",
        "padding": "16px 32px",
        "borderRadius": "xl",
        "fontSize": "body",
        "fontWeight": "600",
        "width": "100%",
        "minHeight": "56px"
      },
      "secondary": {
        "background": "transparent",
        "color": "accent",
        "border": "1px solid accent",
        "padding": "16px 32px",
        "borderRadius": "xl",
        "fontSize": "body",
        "fontWeight": "500"
      },
      "icon": {
        "size": "48px",
        "background": "surface",
        "borderRadius": "full",
        "iconSize": "24px",
        "color": "accent",
        "shadow": "md"
      }
    },
    "bottomNavigation": {
      "height": "80px",
      "background": "surface",
      "borderRadius": "2xl 2xl 0 0",
      "padding": "16px 24px",
      "shadow": "lg",
      "structure": ["navItems"],
      "items": {
        "count": 5,
        "layout": "horizontal",
        "distribution": "space-evenly"
      },
      "navItem": {
        "structure": ["icon", "label"],
        "iconSize": "24px",
        "fontSize": "caption",
        "color": "textMuted",
        "activeColor": "accent",
        "active": {
          "iconSize": "28px",
          "fontWeight": "600"
        }
      }
    },
    "formInput": {
      "background": "transparent",
      "border": "none",
      "borderBottom": "1px solid neutral.border",
      "padding": "12px 0",
      "fontSize": "body",
      "color": "text",
      "placeholder": "textMuted",
      "focus": {
        "borderBottomColor": "accent",
        "outline": "none"
      }
    },
    "authCard": {
      "background": "surface",
      "borderRadius": "2xl",
      "padding": "40px 32px",
      "structure": ["header", "form", "divider", "socialButtons"],
      "header": {
        "fontSize": "display",
        "fontFamily": "display",
        "color": "text",
        "textAlign": "center",
        "marginBottom": "32px",
        "letterSpacing": "0.02em"
      }
    },
    "socialButton": {
      "size": "56px",
      "background": "transparent",
      "border": "1px solid neutral.border",
      "borderRadius": "full",
      "iconSize": "24px",
      "color": "text"
    },
    "productImage": {
      "aspectRatio": "4:3",
      "borderRadius": "2xl 2xl 0 0",
      "overflow": "hidden",
      "objectFit": "cover",
      "background": "secondary.peach"
    },
    "colorSwatch": {
      "size": "56px",
      "borderRadius": "organic",
      "border": "2px solid transparent",
      "overflow": "hidden",
      "active": {
        "border": "2px solid accent",
        "transform": "scale(1.1)"
      },
      "image": {
        "width": "100%",
        "height": "100%",
        "objectFit": "cover"
      }
    },
    "priceTag": {
      "fontSize": "h2",
      "fontWeight": "600",
      "color": "accent",
      "fontFamily": "display"
    },
    "orderSummary": {
      "background": "transparent",
      "padding": "20px 0",
      "borderTop": "1px solid neutral.divider",
      "structure": ["subtotal", "total"],
      "row": {
        "layout": "horizontal",
        "justifyContent": "space-between",
        "padding": "8px 0"
      },
      "label": {
        "fontSize": "body",
        "color": "textSecondary"
      },
      "value": {
        "fontSize": "body",
        "fontWeight": "600",
        "color": "text"
      },
      "total": {
        "fontSize": "h2",
        "fontWeight": "700",
        "color": "accent",
        "marginTop": "8px"
      }
    },
    "heroSection": {
      "structure": ["backgroundImage", "content"],
      "backgroundImage": {
        "position": "absolute",
        "width": "100%",
        "height": "40%",
        "top": "0",
        "objectFit": "cover",
        "opacity": "0.3"
      },
      "botanicalDecoration": {
        "position": "absolute",
        "top": "-20px",
        "right": "-20px",
        "size": "200px",
        "opacity": "0.6"
      }
    }
  },
  "sections": {
    "catalogScreen": {
      "structure": ["header", "filterChips", "productGrid", "bottomNav"],
      "filterChips": {
        "layout": "horizontal-scroll",
        "gap": "8px",
        "padding": "0 20px 16px"
      },
      "productGrid": {
        "display": "grid",
        "columns": "2",
        "gap": "16px",
        "padding": "0 20px"
      }
    },
    "basketScreen": {
      "structure": ["header", "cartItems", "orderSummary", "checkoutButton", "bottomNav"],
      "cartItems": {
        "layout": "vertical",
        "gap": "12px",
        "padding": "20px"
      }
    },
    "authScreen": {
      "structure": ["botanicalDecoration", "authCard"],
      "layout": "centered",
      "padding": "40px 20px"
    },
    "productDetailScreen": {
      "structure": ["heroImage", "productInfo", "colorSelector", "description", "addToCartButton", "bottomNav"],
      "colorSelector": {
        "title": "Select color",
        "layout": "horizontal-scroll",
        "gap": "12px",
        "padding": "20px"
      }
    }
  },
  "states": {
    "hover": {
      "productCard": {
        "transform": "translateY(-4px)",
        "shadow": "lg",
        "transition": "all 0.3s ease"
      },
      "button": {
        "opacity": "0.9",
        "transform": "scale(1.02)"
      },
      "navItem": {
        "transform": "scale(1.1)"
      }
    },
    "active": {
      "filterChip": {
        "background": "botanical.green",
        "color": "surface",
        "border": "1px solid botanical.green"
      },
      "navItem": {
        "color": "accent",
        "iconSize": "28px"
      },
      "colorSwatch": {
        "border": "2px solid accent",
        "transform": "scale(1.1)"
      }
    },
    "focus": {
      "input": {
        "borderBottomColor": "accent",
        "outline": "none"
      }
    },
    "disabled": {
      "button": {
        "opacity": "0.5",
        "cursor": "not-allowed"
      }
    }
  },
  "animations": {
    "transitions": {
      "fast": "150ms cubic-bezier(0.4, 0, 0.2, 1)",
      "normal": "300ms cubic-bezier(0.4, 0, 0.2, 1)",
      "slow": "500ms cubic-bezier(0.4, 0, 0.2, 1)",
      "spring": "400ms cubic-bezier(0.34, 1.56, 0.64, 1)"
    },
    "cardAppear": {
      "keyframes": "fadeInUp",
      "duration": "400ms",
      "easing": "ease-out",
      "delay": "stagger-100ms"
    },
    "buttonPress": {
      "transform": "scale(0.95)",
      "duration": "100ms"
    },
    "pageTransition": {
      "enter": "slideInRight 300ms",
      "exit": "slideOutLeft 300ms"
    }
  },
  "responsive": {
    "breakpoints": {
      "mobile": "428px",
      "tablet": "768px",
      "desktop": "1024px"
    },
    "layout": {
      "mobile": {
        "productGrid": "2-column",
        "padding": "20px",
        "fontSize": "base"
      },
      "tablet": {
        "productGrid": "3-column",
        "padding": "32px",
        "fontSize": "slightly-larger"
      },
      "desktop": {
        "productGrid": "4-column",
        "maxWidth": "1200px",
        "centered": true
      }
    }
  },
  "visualTreatments": {
    "organicShapes": {
      "usage": "product images, color swatches, hero sections",
      "borderRadius": "40% 60% 50% 50% / 60% 40% 60% 40%",
      "variations": [
        "50% 50% 30% 70% / 60% 40% 60% 40%",
        "30% 70% 70% 30% / 30% 30% 70% 70%",
        "60% 40% 50% 50% / 40% 60% 40% 60%"
      ]
    },
    "botanicalElements": {
      "placement": "decorative overlays, background patterns",
      "opacity": "0.1-0.6",
      "usage": "auth screens, empty states, headers"
    },
    "softShadows": {
      "usage": "cards, buttons, floating elements",
      "color": "rgba(45, 95, 95, 0.08)",
      "blur": "20-32px"
    },
    "textureOverlay": {
      "background": "subtle paper texture or grain",
      "opacity": "0.03",
      "blend": "multiply"
    }
  },
  "iconography": {
    "style": "outlined-minimalist",
    "strokeWidth": "1.5px",
    "sizes": {
      "sm": "16px",
      "md": "20px",
      "lg": "24px",
      "xl": "32px"
    },
    "usage": {
      "navigation": "outlined with rounded ends",
      "actions": "simple and clear",
      "decorative": "botanical illustrations"
    }
  },
  "accessibility": {
    "colorContrast": {
      "text": "4.5:1 minimum",
      "largeText": "3:1 minimum",
      "interactive": "3:1 minimum"
    },
    "touchTargets": {
      "minimum": "44px",
      "recommended": "48px",
      "spacing": "8px between targets"
    },
    "focusIndicators": {
      "style": "underline for inputs, outline for buttons",
      "color": "accent",
      "width": "2px"
    },
    "textScaling": {
      "support": "up to 200%",
      "lineHeight": "minimum 1.5"
    }
  },
  "dataStructures": {
    "product": {
      "id": "string",
      "name": "string",
      "description": "string",
      "price": "number",
      "image": "string",
      "colors": "array",
      "metadata": {
        "height": "string",
        "quantity": "number",
        "category": "string"
      }
    },
    "cartItem": {
      "productId": "string",
      "quantity": "number",
      "selectedColor": "string",
      "price": "number"
    },
    "user": {
      "email": "string",
      "name": "string",
      "avatar": "string"
    }
  },
  "brandGuidelines": {
    "tone": "elegant, organic, sophisticated, natural",
    "voice": "refined yet approachable, botanical expertise",
    "imagery": "natural light, soft focus, organic compositions",
    "copywriting": {
      "headlines": "short, elegant, evocative",
      "body": "descriptive, sensory, informative",
      "cta": "gentle encouragement, not pushy"
    }
  }
}